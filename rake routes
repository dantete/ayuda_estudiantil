          edit_session GET    /sessions/:id/edit(.:format)                          sessions#edit
               session GET    /sessions/:id(.:format)                               sessions#show
                       PUT    /sessions/:id(.:format)                               sessions#update
                       DELETE /sessions/:id(.:format)                               sessions#destroy
                  root        /                                                     home#index
